{
  "expo": {
    "name": "Jolly Hour",
    "slug": "jolly-hour-mobile-app",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jollyhour.jollyhourmobileapp",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSCameraUsageDescription": "Usaremos la cámara, ninguna imagen será enviada"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.carloswiboo.jollyhourmobileapp",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/carloswiboo/jolly-hour-mobile-app"
  }
}
